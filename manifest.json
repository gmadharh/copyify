{
  "manifest_version": 3,
  "version": "1.0.0",
  "name": "Spotify Song Extractor",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["scripting"],
  "content_scripts": [
    {
      "matches": [
        "http://open.spotify.com/track/*",
        "https://open.spotify.com/track/*"
      ],
      "js": ["src/script.ts"]
    }
  ]
}
